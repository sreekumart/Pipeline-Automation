pipeline {
    agent any

stages{
        stage('DTLEPS3F'){
            steps {
				sh "echo DTLEPS3F"
				sh "job is executing......"
				sh "sleep 2m"
				sh "execution completed..exiting"
            }
			post {
				failure {
					sh "echo this is post step to trigger mail for fail notification"
				}	
			}
        }
    }
}