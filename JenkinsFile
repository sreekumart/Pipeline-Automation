pipeline {
    agent any

stages{
        stage('DTFILEPP'){
            steps {
                sh "echo DTFILEPP"
            }
        }
		
		stage('DTLEPSKU'){
            steps {
                sh "echo DTLEPSKU"
            }
        }

        stage ('POST-DTLEPSKU'){
            parallel{
                stage ('DTLIBP01'){
                    steps {
                        sh "/c/Users/Stuliche/Desktop/JenkinsFile/Pipeline-Automation/scripts/hello.sh"
                    }
                }

				stage ('DTLEP3SF'){
                    steps {
                        sh "echo DTLEP3SF"
                    }
                }
				
                stage ('DTLEPCOL'){
                    steps {
                        sh "echo DTLEPCOL"
                    }
                }
            }
        }
		
		stage('DTLEPREC'){
		
			parallel{
				stage('DTLEPSEO'){
					steps {
						sh "echo DTLEPSEO"
					}
					post {
						success {
							build (job: 'DTPPENDBRANCH-2', wait: false)
						}
					}

				}
				
				stage('EXT-DTLEPLOT'){
					steps{
						build (job: 'DTPPENDBRANCH-1', wait: false)
					}
				}
			}
		
		
		}
    }
}